begin
  const achar = 97;
  var c;
  var i;
  begin
    i := 0;
    while i < 10
    do
      c := achar + i;
      % prints 98, 99, 100, 101, 102, 103, 104, 105, 106, 107
      print c+1;
      i := i+1
    end
  end
end.
