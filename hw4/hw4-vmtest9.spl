begin
  const q = 7;
  var x, y, z;
  begin
    x := 86;
    print x;     % prints 86
    y := 99;
    print y;     % prints 99
    z := 1001;
    print z;     % prints 1001
    print (x-y); % prints -13
    if divisible (x-y) by 2
    then
      y := y-1*z;
      print y
    else
      x := z+1+q;
      print x    % prints 1009
    end
  end
end.
