begin
  const zero = 0, one = 1, two = 2;
  const three = 3;
  var v1, v2;
  var v3, v4;
  print zero;  % prints 0
  print one;   % prints 1
  print two;   % prints 2
  v1 := one;  
  print v1;    % prints 1
  v2 := two / one;
  print v2;    % prints 2
  v3 := (two + 1 + 0) / one;
  print v3     % prints 3
end.
